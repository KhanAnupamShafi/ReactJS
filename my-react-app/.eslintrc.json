{
  "env": {
    "browser": true,
    "es2021": true
  },

  "extends": [
    "plugin:react/recommended",
    "eslint:recommended",
    //  "airbnb",
    "prettier",
    "plugin:prettier/recommended"
  ],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 12,
    "sourceType": "module"
  },

  "plugins": ["react", "prettier", "jsx-a11y", "import"],

  "parser": "babel-eslint",

  "rules": {
    "no-unused-vars": "warn",
    "react/jsx-uses-react": "error",
    "react/jsx-uses-vars": "error",
    "no-console": "warn",
    "import/first": "error",
    "react/prop-types": 0,
    "linebreak-style": ["error", "unix"]
  }
}
